      <_reset>15</_reset>
      <_options>
        <dialogueOption>
          <_text />
          <_dest>-1</_dest>
        </dialogueOption>
      </_options>
      <_precalls />
      <_postcalls />
    </Node>
    <Node>
      <_ID>16</_ID>
      <_text>They aren’t lousy-! Th-they’re just busy, that’s all… </_text>
      <_name>Ms. Teacup</_name>
      <_reset>0</_reset>
      <_options>
        <dialogueOption>
          <_text />
          <_dest>17</_dest>
        </dialogueOption>
